{
    "require": {
        "php": ">=8.1.0",
        "openai-php/client": "^0.7.4",
        "symfony/http-client": "^6.4",
        "nyholm/psr7": "^1.8",
        "rajentrivedi/tokenizer-x": "^1.0",
        "league/pipeline": "^1.0",
        "laravel/prompts": "^0.1.23",
        "minicli/minicli": "^4.2",
        "minicli/command-help": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-0": {
            "service\\": [""]
        },
        "psr-4": {
            "Command\\": ["Command"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "test\\": [""]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
